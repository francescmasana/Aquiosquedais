-content_for :dyn_js do
	:plain
		<script type="text/javascript">
			$(document).ready(function() {
			    jQuery('a.popup').live('click', function(){
				    newwindow=window.open($(this).attr('href'),'','height=350,width=750');
				    if (window.focus) {newwindow.focus()}
				    return false;
			    });
			});
		</script>
		
